{
    "name": "TestUser",
    "description": "testUser hasMany relation",
    "base": "PersistedModel",
    "idInjection": true,
    "properties": {
        "username":{
            "type": "string",
            "required": true
        }
    },
    "relations": {
        "todos": {
            "type": "hasMany",
            "model": "Todo",
            "foreignKey": "userId"
        },
        "testtodos": {
            "type": "hasMany",
            "model": "TestTodo",
            "foreignKey": "testUserId"
        }
    }
}
